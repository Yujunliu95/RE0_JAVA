public class Demo07Operator{
    public static void main (String[] args) {
        int a = 10;
        a += 5;
        System.out.println(a); //15

        int x = 10;
        x %= 3;
        System.out.println(x); // 1

        byte num = 30;
        // num = byte + int
        // num = int+ int
        // num = (byte) int
        num += 5;












    }
}